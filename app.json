{
  "expo": {
    "name": "SmartCart",
    "slug": "SmartCart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoApp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ralphs.ecommerce"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/Fonts/OpenSans-Bold.ttf",
            "./assets/Fonts/OpenSans-Italic.ttf",
            "./assets/Fonts/OpenSans-Medium.ttf",
            "./assets/Fonts/OpenSans-Regular.ttf",
            "./assets/Fonts/ProximaNova-Light.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7db8c2a9-0ade-4555-8334-bca79db3aec0"
      }
    }
  }
}
