{
  "compilerOptions": {
    "target": "esnext",
    "module": "CommonJS",
    "lib": ["esnext", "dom"],
    "jsx": "react-native",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "**/*.config.js"],
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
  "extends": "expo/tsconfig.base"
}
